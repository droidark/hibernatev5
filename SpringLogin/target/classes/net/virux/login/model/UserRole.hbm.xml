<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="net.virux.login.model.UserRole" table="user_roles" catalog="virux">
		<id name="userRoleId" type="java.lang.Integer">
			<column name="user_role_id"/>
			<generator class="identity"/>
		</id>
		<!-- RELACIÃ“N -->
		<many-to-one name="user" class="net.virux.login.model.User" fetch="select">
			<column name="username" length="45" not-null="true"></column>
		</many-to-one>
		<property name="role" type="java.lang.String">
			<column name="role" length="45" not-null="true"/>
		</property>
	</class>
</hibernate-mapping>